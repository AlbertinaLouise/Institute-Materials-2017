   "source": [
    "from lxml import etree\n",
    "\n",
    "def tei(tag):\n",
    "    return \"{http://www.tei-c.org/ns/1.0}%s\" % tag\n",
    "\n",
    "\n",
    "with open('ozymandias.xml', encoding='utf-8') as f:\n",
    "    ozzy = etree.parse(f)\n",
    "    \n",
    "    \n",
    "print(etree.tostring(ozzy).decode('utf-8'))\n",
    "\n",
    "\n"
   ]
